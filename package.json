{
  "name": "electron-angular-native",
  "version": "2.0.0-beta",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "build": "ng build --prod && npm run compile:main:dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:main:serve": "tsc src/main.electron.ts --outDir serve",
    "compile:main:dist": "tsc src/main.electron.ts --outDir dist",
    "serve": "npm run compile:main:serve && concurrently \"ng build --watch --output-path=serve/\" \"electron ./ --serve\"",
    "build:prod": "cross-env NODE_ENV=production npm run build && cpy app.package.json dist --rename=package.json",
    "package:windows": "npm run build:prod && npx electron-builder build --windows"
  },
  "main": "serve/main.electron.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.4.1",
    "electron": "^1.8.2",
    "ffi": "^2.2.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.8",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/ffi": "^0.1.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.1.3",
    "electron-builder": "^19.56.0",
    "electron-reload": "^1.2.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
